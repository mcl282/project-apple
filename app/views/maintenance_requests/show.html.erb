<h1>MaintenanceRequests#show</h1>
<p>Find me in app/views/maintenance_requests/show.html.erb</p>
<% @maintenance_request.request_message %>


<% @maintenance_requests.each do |rqst| %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= "#{@tenant.email} submitted #{rqst.created_at.strftime("%B %e, %Y at %I:%M %p")}" %></h3>
  </div>
  <div class="panel-body">
    <%= rqst.request_message %>
  </div>
</div>


  
<% end %>
